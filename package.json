{
  "name": "appbase",
  "version": "1.0.0",
  "description": "A full-featured PWA template with framework7, webpack, hot-reload, lint-on-save, unit testing & css extraction.",
  "author": "Jason Cheng <Jasoncckit@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jasoncckit/Appbase.git/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jasoncckit/Appbase.git"
  },
  "keywords": [
    "ios",
    "browserify",
    "framework7",
    "f7",
    "vue",
    "vues",
    "material",
    "mobile",
    "app"
  ],
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
